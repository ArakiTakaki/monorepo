{
  "name": "hack-and-srash",
  "packageManager": "yarn@3.5.1",
  "scripts": {
    "postinstall": "husky install",
    "dev": "vite",
    "build": "tsc && vite build",
    "check:code": "yarn eslint ./src",
    "check:style": "stylelint ./src/**/*.scss",
    "check:spell": "cspell ./src",
    "format:code": "eslint --fix && yarn prettier --write './**/*.{js,jsx,ts,tsx,json}'",
    "format:style": "stylelint --fix ./src/**/*.scss",
    "test": "vitest",
    "preview": "vite preview",
    "commit": "git-cz"
  },
  "volta": {
    "node": "18.16.0",
    "yarn": "3.5.1"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "@tanstack/react-query": "^4.29.12",
    "@tanstack/react-query-devtools": "^4.29.12",
    "@tanstack/router": "^0.0.1-beta.53",
    "pixi.js": "^7.2.4",
    "postcss": "^8.4.24",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.7",
    "redux-logger": "^3.0.6",
    "ress": "^5.0.2"
  },
  "devDependencies": {
    "@tanstack/router-devtools": "^0.0.1-alpha.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.4",
    "@types/redux-logger": "^3.0.9",
    "@types/testing-library__jest-dom": "^5.14.6",
    "@types/testing-library__user-event": "^4.2.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/ui": "^0.31.4",
    "autoprefixer": "^10.4.14",
    "cspell": "^6.31.1",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.32.2",
    "git-cz": "^4.9.0",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^13.2.2",
    "msw": "^1.2.1",
    "prettier": "^2.8.8",
    "sass": "^1.62.1",
    "stylelint": "^15.6.2",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-standard-scss": "^9.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-rscss": "^0.4.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.9",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.31.4"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
