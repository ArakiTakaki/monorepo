{
  "name": "core",
  "packageManager": "yarn@3.5.1",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest --run --coverage",
    "check:all": "npm-run-all --parallel check:test check:code check:style check:spell check:text",
    "check:test": "vitest --run",
    "check:code": "eslint ./src",
    "check:style": "stylelint ./src/**/*.scss",
    "check:text": "textlint ./",
    "format:all": "npm-run-all format:code format:style format:text",
    "format:code": "eslint --fix && yarn prettier --write './**/*.{js,jsx,ts,tsx,json}'",
    "format:style": "stylelint --fix ./src/**/*.scss",
    "format:text": "textlint --fix ./"
  },
  "volta": {
    "node": "18.16.0",
    "yarn": "3.5.1"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "@tanstack/react-query": "^4.29.12",
    "@tanstack/router": "^0.0.1-beta.53",
    "@vitest/coverage-istanbul": "^0.31.4",
    "pixi.js": "^7.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.7",
    "ress": "^5.0.2"
  },
  "devDependencies": {
    "@tanstack/react-query-devtools": "^4.29.12",
    "@tanstack/router-devtools": "^0.0.1-alpha.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.4",
    "@types/redux-logger": "^3.0.9",
    "@types/testing-library__jest-dom": "^5.14.6",
    "@types/testing-library__user-event": "^4.2.0",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/ui": "^0.31.4",
    "@workspaces/eslint-config": "workspace:^",
    "@workspaces/prettier-config": "workspace:^",
    "@workspaces/stylelint-config": "workspace:^",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.42.0",
    "git-cz": "^4.9.0",
    "jsdom": "^22.1.0",
    "msw": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "redux-logger": "^3.0.6",
    "sass": "^1.62.1",
    "textlint": "^13.3.2",
    "textlint-rule-preset-japanese": "^7.0.0",
    "textlint-rule-preset-jtf-style": "^2.3.13",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.31.4"
  },
  "browserslist": [
    "last 2 version",
    "iOS >= 8.1",
    "Android >= 4.4"
  ],
  "msw": {
    "workerDirectory": "public"
  }
}
